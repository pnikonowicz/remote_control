{
  "manifest_version": 2,
  "name": "Remote Control",
  "version": "1.0",

  "description": "Provides default behavior for media players",

  "icons": {
  },

  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["remote_control.js"]
    }
  ]
}

